<thead>
  <tr>        

              <th>User Name</th>
              <th>User Email</th>
              <th></th>    
  </tr>
  </thead>
  <tbody>

        <% @nominees.each do |nominee| %>
        
        <tr>       

          <td><%= nominee.fullname %></td>
