
<%= javascript_include_tag "jquery-1.5.2.min","validation" %>
<%= form_for @nominee ,:html => {:id =>"nominee"} do |f| %>
  <% if @nominee.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@nominee.errors.count, "error") %> prohibited this nominee from being saved:</h2>

      <ul>
      <% @nominee.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

 
  <div class="field">
      <br><%= f.label :Start_Cycle %>
      <%= f.collection_select :start_cycle, VoteSetting.find_all_by_user_id(current_user.id) , :start_cycle,:start_cycle %> 
      <%= f.label :End_Cycle %>
      <%= f.collection_select :end_cycle, VoteSetting.find_all_by_user_id(current_user.id) , :end_cycle,:end_cycle %></br>   
  </div> 

 <!--  <div class="field">
      <br><%= f.label "vote_setting_id" %><br />
      <%= f.collection_select :vote_setting_id, VoteSetting.all,:start_cycle,:end_cycle %></br>    
  </div> -->



  <div class="field">
      <br><%= f.label "Name" %><br />
      <%= f.collection_select :fullname, User.find_all_by_admin_user_id(current_user.id), :fullname,:fullname %></br>    
  </div>



  <div class="field">
      <br><%= f.label "Email" %><br />
      <%= f.collection_select :email, User.find_all_by_admin_user_id(current_user.id), :email,:email %></br>      
  </div> 



  </br><div class="actions">
    <%= f.submit %>
  </div>
<% end %>
