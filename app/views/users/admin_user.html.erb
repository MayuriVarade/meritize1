
<%= stylesheet_link_tag "jquery-ui-1.8.16.custom.css" %>

<script>
var data = <%= raw @searchuser %>;

$.noConflict();
jQuery(function() {

  jQuery( "#search" ).autocomplete(
  {
     source:data
  });
    
});
</script>



<div id="masthead">
  
  <div class="container">
    
    <div class="masthead-pad">
      
      <div class="masthead-text">
        <h2>Manage User</h2>
        
      </div> 
      
    </div>
    
  </div>
  
</div> 




<div id="content">

<div class="container">
    <section class="round">
        <% flash.each do |key, value| %>
      <div class="flash <%= key %>" style = "font-size:15px;text-align:center;"><%= value %></div>
      <% end %>
  </section>

  <div style="margin-bottom:10px"> <%= link_to "Add new User" , new_user_path(:type =>"adminuser"),:class=>"btn btn-primary " %></div>
   <%= form_tag admin_user_path, :method => 'get',:html =>{:id =>"edit-profile",:class=> "form-horizontal"}  do %>
     <%= text_field_tag :search, params[:search], :class=>"input-medium" %>
      <%= submit_tag "Search", :class_name => nil,:class=> "btn btn-primary",:style=> "margin-bottom:10px; padding:5px 10px" %>
   
   <% end %>

        
     
      
       <table class="table table-bordered table-striped table-highlight">
          <thead>
            <tr>
              
              <th>First Name</th>
              <th>Last Name</th>
              <th colspan="3">Action</th>
            </tr>
          </thead>
          <% @adminusers.each do |adminusers| %>
          <tbody>
            <tr>
              
              <td><%= adminusers.firstname rescue nil %></td>
              <td><%= adminusers.lastname rescue nil%></td>
              <td><%= link_to 'Edit', edit_user_path(adminusers,:type=>"adminuser") %></td>
              <td><a href="/users/<%= adminusers.id %>/delete" onclick="if(confirm('Are you sure you want to delete this page?'))  return true;  else  return false;" class = "icon-3 info-tooltip" >Delete</a></td>

              <td><%= link_to format_boolean(adminusers.status?), {:controller => 'users', :action => 'toggled_status', :id => adminusers.id} %></td>
            </tr>
             
          </tbody>
          <% end %>
        </table>
       
      </div> 

</div> 

</div> 
</div>
