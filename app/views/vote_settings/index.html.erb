<h1>Listing vote settings</h1>

<table>
  <tr>
    <th>Award program name</th>
    <th>Award frequency type</th>
    <th>Start cycle</th>
    <th>End cycle</th>
    <th>Intro text</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vote_settings.each do |vote_setting| %>
  <tr>
    <td><%= vote_setting.award_program_name %></td>
    <td><%= vote_setting.award_frequency_type %></td>
    <td><%= vote_setting.start_cycle %></td>
    <td><%= vote_setting.end_cycle %></td>
    <td><%= vote_setting.intro_text %></td>
    <td><%= link_to 'Show', vote_setting %></td>
    <td><%= link_to 'Edit', edit_vote_setting_path(vote_setting) %></td>
    <td><%= link_to 'Destroy', vote_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vote setting', new_vote_setting_path %>
